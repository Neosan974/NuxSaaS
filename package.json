{
  "name": "nuxsaas",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "test": "vitest run --silent=false",
    "coverage": "vitest run --coverage",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "auth:schema": "npx @better-auth/cli generate --config server/utils/auth.ts --output server/database/schema/auth.ts -y",
    "db:generate": "drizzle-kit generate --config ./server/database/drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config ./server/database/drizzle.config.ts",
    "build": "nuxt build",
    "serve": "node .output/server/index.mjs",
    "deploy": "wrangler deploy",
    "prepare": "husky"
  },
  "dependencies": {
    "@better-auth/stripe": "^1.2.9",
    "@nuxt/ui": "^3.1.3",
    "@nuxthub/core": "^0.9.0",
    "@nuxtjs/i18n": "^9.5.5",
    "@nuxtjs/seo": "^3.0.3",
    "@vueuse/core": "13.3.0",
    "@vueuse/integrations": "^13.3.0",
    "better-auth": "^1.2.9",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "drizzle-orm": "0.44.2",
    "ioredis": "^5.6.1",
    "nuxt": "^3.17.5",
    "nuxt-charts": "^0.1.9",
    "ofetch": "^1.4.1",
    "pg": "^8.16.0",
    "resend": "^4.5.2",
    "sortablejs": "^1.15.6",
    "stripe": "^18.2.1",
    "uuid": "^11.1.0",
    "vue": "^3.5.16",
    "vue-component-type-helpers": "^2.2.10",
    "zod": "^3.25.63"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.14.1",
    "@better-auth/cli": "^1.2.9",
    "@iconify-json/lucide": "^1.2.48",
    "@iconify-json/simple-icons": "^1.2.38",
    "@nuxt/eslint": "^1.4.1",
    "@nuxt/test-utils": "^3.19.1",
    "@types/pg": "^8.15.4",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/coverage-v8": "^3.2.3",
    "@vue/test-utils": "^2.4.6",
    "drizzle-kit": "^0.31.1",
    "eslint": "^9.28.0",
    "happy-dom": "^17.6.3",
    "husky": "^9.1.7",
    "playwright-core": "^1.53.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.3",
    "wrangler": "^4.20.0"
  }
